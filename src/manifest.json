{
  "name": "Tab Mom",
  "version": "0.1.0.1",
  "description": "Tab Mom",
  "permissions": [
    "tabs", "alarms", "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Tab Mom",
    "default_icon": "icons/tabmom.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/tabmom.png",
    "48": "icons/tabmom48.png",
    "128": "icons/tabmom128.png" 
  },
  "manifest_version": 2
}
